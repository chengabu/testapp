<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.moblie.withoutapi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.Manifest.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.moblie.withoutapi.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.moblie.withoutapi.HttpRequestWithoutAPI"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.moblie.withoutapi.AsyncUpdateHttpRequestWithoutAPI"
            android:label="@string/app_name" >
        </activity>

        <provider
            android:name="com.example.testapplibrary.database.HttpRequstResultProvider"
            android:authorities="com.dell.test.withoutapi.http"
            android:exported="true"
            android:label="Http Request Provider"
            android:multiprocess="false" />
    </application>

</manifest>